{
    "verbose": true,
    "collectCoverageFrom": [
        "!websockets/*.js",
        "api/**",
        "!api/routes.js",
        "!api/**/router.js",
        "!utils/*.js",
        "!middleware/*.js"
    ],
    "coverageThreshold": {
        "global": {
            "lines": 90,
            "statements": 90
        }
    }
}